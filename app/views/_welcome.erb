<% if current_user %>
  Welcome, <%= current_user.name %>
  <form method='post' action='/sessions'>
    <input type='hidden' name='_method' value='delete'>
    <input type='submit' value='Sign out'>
  </form>
<% else %>
  <a href='/users/new'>Sign Up</a> <a href='/sessions/new'>Sign In</a>
<% end %>
